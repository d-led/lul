# GNU Make solution makefile autogenerated by Premake
# Type "make help" for usage help

ifndef config
  config=debug
endif
export config

PROJECTS := luabind lul_example

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

luabind: 
	@echo "==== Building luabind ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f luabind.make

lul_example: luabind
	@echo "==== Building lul_example ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f lul_example.make

clean:
	@${MAKE} --no-print-directory -C . -f luabind.make clean
	@${MAKE} --no-print-directory -C . -f lul_example.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   luabind"
	@echo "   lul_example"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
